
SHELL=@SHELL@

MKDIR=@top_srcdir@/mkinstalldirs
INSTALL=@INSTALL@
INSTALLDATA=@INSTALL_DATA@

.PHONY: install

install:
	$(MKDIR) $(INSTDIR)/web_lib
	$(MKDIR) $(INSTDIR)/web_sys
	$(MKDIR) $(INSTDIR)/web_demo_lib
	$(MKDIR) $(INSTDIR)/www
	$(MKDIR) $(INSTDIR)/www/web
	$(MKDIR) $(INSTDIR)/www/web/employee
	$(MKDIR) $(INSTDIR)/www/web/link
	$(MKDIR) $(INSTDIR)/www/web/rating
	$(MKDIR) $(INSTDIR)/www/web/secret
	$(MKDIR) $(INSTDIR)/www/web/upload
	$(MKDIR) $(INSTDIR)/www/web/upload/files
	$(INSTALLDATA) web_lib/*.{sml,mlb} $(INSTDIR)/web_lib
	$(INSTALLDATA) web_sys/*.sml $(INSTDIR)/web_sys
	$(INSTALLDATA) www/web.mlb $(INSTDIR)/www
	$(INSTALLDATA) www/Makefile $(INSTDIR)/www
	$(INSTALLDATA) www/web/*.{sml,msp,html} $(INSTDIR)/www/web
	$(INSTALLDATA) www/web/Makefile $(INSTDIR)/www/web
	$(INSTALLDATA) www/web/employee/*.{sql,sml} $(INSTDIR)/www/web/employee
	$(INSTALLDATA) www/web/link/*.sml $(INSTDIR)/www/web/link
	$(INSTALLDATA) www/web/rating/*.{sql,sml,jpg} $(INSTDIR)/www/web/rating
	$(INSTALLDATA) www/web/secret/*.sml $(INSTDIR)/www/web/secret
	$(INSTALLDATA) www/web/upload/*.sml $(INSTDIR)/www/web/upload
	$(INSTALLDATA) www/web/upload/files/*.png $(INSTDIR)/www/web/upload/files
	$(INSTALLDATA) web_demo_lib/*.sml $(INSTDIR)/web_demo_lib

